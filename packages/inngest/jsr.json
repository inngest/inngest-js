{
  "$schema": "https://jsr.io/schema/config-file.v1.json",
  "name": "@inngest/sdk",
  "description": "Official SDK for Inngest.com. Inngest is the reliability layer for modern applications. Inngest combines durable execution, events, and queues into a zero-infra platform with built-in observability.",
  "version": "3.19.18",
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "./src/test/**",
    "./test/**",
    "*.js",
    "./scripts/**",
    "./**/*.test.*",
    "./**/tsconfig.*",
    "!./src/version.ts"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./astro": "./src/astro.ts",
    "./bun": "./src/bun.ts",
    "./cloudflare": "./src/cloudflare.ts",
    "./digitalocean": "./src/digitalocean.ts",
    "./edge": "./src/edge.ts",
    "./express": "./src/express.ts",
    "./fastify": "./src/fastify.ts",
    "./h3": "./src/h3.ts",
    "./koa": "./src/koa.ts",
    "./lambda": "./src/lambda.ts",
    "./next": "./src/next.ts",
    "./nuxt": "./src/nuxt.ts",
    "./redwood": "./src/redwood.ts",
    "./remix": "./src/remix.ts",
    "./sveltekit": "./src/sveltekit.ts",
    "./deno/fresh": "./src/deno/fresh.ts",
    "./hono": "./src/hono.ts"
  }
}